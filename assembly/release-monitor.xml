<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>dubbokeeper-monitor</id>
    <baseDirectory>/</baseDirectory>
    <formats>
        <format>dir</format>
        <format>tar.gz</format>
    </formats>
    <fileSets>
        <fileSet>
            <includes>
                <include>bin/*monitor.*</include>
                <include>bin/setting-monitor.*</include>
                <include>conf/*monitor.properties</include>
                <include>conf/logback.xml</include>
            </includes>
        </fileSet>
    </fileSets>
    <moduleSets>
        <moduleSet>
            <includes>
                <include>com.dubboclub.dk:dubbokeeper-monitor</include>
            </includes>
            <binaries>
                <outputDirectory>lib/</outputDirectory>
                <unpack>false</unpack>
                <dependencySets>
                    <dependencySet>
                        <outputDirectory>lib/</outputDirectory>
                    </dependencySet>
                </dependencySets>
            </binaries>
        </moduleSet>
    </moduleSets>
</assembly>
